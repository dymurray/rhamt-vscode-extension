      },
      {
        "name": "skipReports",
        "description": "If set, reports will not be generated.",
        "type": "Boolean",
        "ui-type": ["single"],
        "required": false
      },
      {
        "name": "overwrite",
        "description": "Force overwrite of the output directory, without prompting.",
        "type": "Boolean",
        "ui-type": ["single"],
        "required": false
      },
      {
        "name": "output",
        "description": "The location of the generated analysis results.",
        "type": "String",
        "ui-type": ["single"],
